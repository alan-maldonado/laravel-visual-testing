{
    "name": "alan-maldonado/laravel-visual-testing",
    "description": "Visual UI testing for Laravel Dusk using percy.io",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Joseph Szobody",
            "email": "joseph@stechstudio.com"
        }
    ],
    "require": {
        "laravel/dusk": "^8.2"
    },
    "require-dev": {
        "orchestra/testbench": "^9.5",
        "phpunit/phpunit": "^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "STS\\VisualTesting\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "STS\\VisualTesting\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "STS\\VisualTesting\\VisualTestingServiceProvider"
            ]
        }
    }
}
